{
  "include": [
    "server/**/*.ts",
    "common/**/*.ts"
  ],
  "extension": [
    ".ts",
    ".tsx"
  ],
  "exclude": [
    "**/*.d.ts"
  ],
  "require": [
    "ts-node/register"
  ],
  "reporter": [
    "text-summary",
    "json-summary",
    "html"
  ],
  "check-coverage": true,
  "branches": 80,
  "functions": 80,
  "lines": 80,
  "statements": 80,
  "sourceMap": true,
  "instrument": true,
  "report-dir": ".coverage",
  "cache": true,
  "all": true
}